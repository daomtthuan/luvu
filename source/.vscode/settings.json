{
  "editor.wordWrapColumn": 120,
  "html.format.wrapLineLength": 120,
  "editor.rulers": [120],
  "prettier.printWidth": 120,
  "i18n-ally.annotationMaxLength": 120,
  "importSorter.importStringConfiguration.maximumNumberOfImportExpressionsPerLine.count": 120,
  "cSpell.words": ["daomtthuan", "luvu", "Pressable"],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.*": true,
    "**/node_modules": true,
    "**/yarn.lock": true,
    "**/babel.config.js": true,
    "**/metro.config.js": true
  },
  "explorerExclude.backup": null,
  "exportall.config.folderListener": [
    "/app/components/global/Button",
    "/app/components/global/View",
    "/app/components/global"
  ]
}
